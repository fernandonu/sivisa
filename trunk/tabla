CREATE SCHEMA epi
  AUTHORIZATION postgres;
 
 CREATE TABLE epi.veterinarias
(
  id_veterinaria serial NOT NULL,
  nom_veterinaria character varying(250) NOT NULL DEFAULT 250,
  domicilio character varying(250),
  numero integer,
  localidad character varying(250),
  departamento character varying(200),
  CONSTRAINT veterinarias_pkey PRIMARY KEY (id_veterinaria)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.veterinarias OWNER TO postgres;


CREATE TABLE epi.denuncia
(
  id_denuncia serial NOT NULL,
  n_prof character varying(200) NOT NULL,
  a_prof character varying(200) NOT NULL,
  matricula character varying(8),
  dni_prof integer NOT NULL,
  tabla character varying(150) NOT NULL,
  fecha_notif date NOT NULL,
  CONSTRAINT denuncia_pkey PRIMARY KEY (id_denuncia)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.denuncia OWNER TO postgres;

 
  CREATE TABLE epi.brucel_can
(
  id_bruc_can integer NOT NULL DEFAULT nextval('epi.lvcan_id_lvc_seq'::regclass),
  id_denuncia integer NOT NULL,
  usuario character varying(50),
  fecha_carga timestamp(6) without time zone,
  raza character varying(200),
  sexo character varying(10),
  color_m character varying(100),
  edad integer,
  nombre character varying(200),
  dato_epidem text,
  laboratorios text,
  CONSTRAINT brucel_can_pkey PRIMARY KEY (id_bruc_can)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.brucel_can OWNER TO postgres;

CREATE TABLE epi.brucellosis
(
  id_bucelosis serial NOT NULL,
  id_denuncia integer NOT NULL,
  ape_pac character varying(250),
  nom_pac character varying(250),
  f_nacimiento date,
  sexo character(4),
  domicilio character varying(300),
  localidad character varying(300),
  departamento character varying(300),
  dias_com character varying(10),
  subito character(4),
  insidioso character(4),
  desc_clinica text,
  terap_esp text,
  primera_dosis date,
  ultima_dosis date,
  prev_diag character(4),
  primer_diag date,
  direc_fdiag character varying(350),
  f_huddlesson date,
  res_huddlesson text,
  lab_huddlesson text,
  f_tsinme date,
  res_tsinme text,
  lab_tsinme text,
  f_tconme date,
  res_tconme text,
  lab_tconme text,
  f_rbengala date,
  res_rbengala text,
  lab_rbengala text,
  f_fcomplem date,
  res_fcomplem text,
  lab_fcomplem text,
  f_pcombs date,
  res_pcombs text,
  lab_pcombs text,
  dom_t character varying(350),
  oc_previa character varying(350),
  contacto_animal character(4),
  esp_bovino character varying(300),
  esp_cerdo character varying(300),
  esp_cabras character varying(300),
  esp_otros character varying(300),
  vac_antibrucelosa character(4),
  leche character(4),
  leche_cruda character(4),
  obs text,
  CONSTRAINT brucellosis_pkey PRIMARY KEY (id_bucelosis)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.brucellosis OWNER TO postgres;

CREATE TABLE epi.hidatidosis
(
  id_hidat serial NOT NULL,
  ap_pac character varying(200),
  nom_pac character varying(200),
  sexo_pac character varying(10),
  f_nac date,
  domicilio character varying(200),
  localidad character varying(200),
  departamento character varying(200),
  rural character varying(300),
  domestica character varying(300),
  profesional character varying(300),
  otros character varying(300),
  mn_unoh integer,
  mn_unom integer,
  mna_qh integer,
  mna_qm integer,
  my_qh integer,
  my_qm integer,
  cant_perros integer,
  p_comen text,
  ovino integer,
  bovino integer,
  pocino integer,
  equino integer,
  f_sintoma date,
  descrip text,
  tmedico character varying(300),
  tquirurgico character varying(300),
  dd5 character varying(300),
  contraief character varying(300),
  inmunoef character varying(300),
  ecografia text,
  tac text,
  f_notificacion date,
  medidas text,
  obs text,
  CONSTRAINT hidatidosis_pkey PRIMARY KEY (id_hidat)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.hidatidosis OWNER TO postgres;

CREATE TABLE epi.leptospirosis
(
  id_leptosp serial NOT NULL,
  id_denuncia integer NOT NULL,
  ape_pac character varying(250),
  nom_pac character varying(250),
  f_nacimiento date,
  sexo character(4),
  domicilio character varying(300),
  localidad character varying(300),
  departamento character varying(300),
  trurales character(4),
  e_frogorifico character(4),
  obrero character varying(350),
  otro character varying(350),
  f_psintoma date,
  f_internacion date,
  f_muestra date,
  ictericia character(4),
  cefalea character(4),
  s_mengeo character(4),
  iconjuntivalbilat character(4),
  fiebre character(4),
  mialgias character(4),
  ers1hs character varying(50),
  leucositosis character varying(50),
  eutrofilia character varying(50),
  uremia character varying(50),
  bili_direc character varying(50),
  tgp character varying(50),
  cpk character varying(50),
  a_domestico character(4),
  roedores character(4),
  rio_arroyo character(4),
  laguna character(4),
  alcantarilla character(4),
  inundacion character(4),
  f_notificacion date,
  semana_epi character varying(200),
  esablecimiento character varying(350),
  serologia character(4),
  positividad character(4),
  titulo text,
  aislamiento character(4),
  obs text,
  CONSTRAINT leptospirosis_pkey PRIMARY KEY (id_leptosp)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.leptospirosis OWNER TO postgres;


CREATE TABLE epi.lvcan
(
  id_lvc serial NOT NULL,
  id_denuncia integer NOT NULL,
  usuario character varying(50),
  "fecha de carga" timestamp without time zone,
  raza character varying(200),
  sexo character varying(10),
  color_m character varying(100),
  edad integer,
  nombre character varying(200),
  cri_flia character(4),
  calle character(4),
  refugio character(4),
  importacion character(4),
  prov_nac character varying(200),
  callejero character(4),
  int_casa character(4),
  gallinero character(4),
  m_perros character(4),
  cant integer,
  problema character(4),
  lab_fecha date,
  sangre character(4),
  suero character(4),
  ganglio character(4),
  piel character(4),
  otro character(4),
  parasitologico character varying(200),
  paras_res character varying(100),
  serologico character varying(200),
  serol_res character varying(100),
  molecular character varying(200),
  mol_res character varying(100),
  nom_prop character varying(200),
  ape_prop character varying(200),
  dni_prop integer,
  dom_prop character varying(300),
  nro_prop character varying(30),
  tel character varying(200),
  loca_prop character varying(300),
  dep_prop character varying(300),
  prop_tenedor character(4),
  CONSTRAINT lvcan_pkey PRIMARY KEY (id_lvc)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE epi.lvcan OWNER TO postgres;

